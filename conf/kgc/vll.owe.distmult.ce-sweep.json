{
  "general": {
    "seed": null
  },
  "tracker": {
    "cls": "wandb",
    "project": "irtm-keen",
    "reinit": true,
    "offline": false
  },
  "model": {
    "cls": "DistMult",
    "embedding_dim": {
      "low": 50,
      "high": 500,
      "step": 50,
      "log": false,
      "initial": 250
    },
    "preferred_device": "cuda"
  },
  "optimizer": {
    "cls": "Adagrad",
    "lr": {
      "low": 0.001,
      "high": 1,
      "step": null,
      "log": true,
      "initial": 0.1
    }
  },
  "evaluator": {
    "cls": "RankBasedEvaluator"
  },
  "regularizer": {
    "cls": "LpRegularizer",
    "p": 2.0,
    "weight": {
      "low": 0.001,
      "high": 0.1,
      "step": null,
      "log": true,
      "initial": null
    },
    "normalize": true
  },
  "loss": {
    "cls": "CrossEntropyLoss"
  },
  "stopper": {
    "cls": "EarlyStopper",
    "frequency": 10,
    "patience": 10,
    "relative_delta": 0.00001,
    "metric": "hits_at_k",
    "larger_is_better": true
  },
  "sampler": {
    "cls": "BasicNegativeSampler",
    "num_negs_per_pos": {
      "low": 1,
      "high": 50,
      "step": 1,
      "log": false,
      "initial": 25
    }
  },
  "training": {
    "num_epochs": 2000,
      "batch_size": 500
  },
  "training_loop": {
    "cls": "LCWATrainingLoop"
  },
  "optuna": {
    "trials": 50,
    "maximise": true
  }
}
